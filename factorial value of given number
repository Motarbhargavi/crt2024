#include <stdio.h>

int main() {
    // Write C code here
   int n;
   scanf("%d",&n);
   while(n>=n){
       printf("%d",n);
       n=n-1;
}
}
       
      (or)

      #include <stdio.h>

int main() {
    // Write C code here
   int n,r=1;
   scanf("%d",&n);
   while(n>=1){
       r=r*n;
       n=n-1;
   }
       printf("%d",r);
}
