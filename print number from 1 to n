// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    void fun(int n)
    {
        if(n>=1)
        {
            fun(n-1);
                        printf("%d",n);

        }
    }
    int main()
    {
        int n;
        scanf("%d",&n);
        fun(n);
    }
}
