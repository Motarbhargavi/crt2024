#include <stdio.h>
#include <math.h>
int main() {
    // Write C code here
    int n,r;
    scanf("%d",&n);
    r=log10(n)+1;
    printf("%d",r);
}



(or)
#include <stdio.h>
#include <math.h>
int main() {
    // Write C code here
    int n,d,s=0;
    scanf("%d",&n);
    while (n>0)
    {
        d=n%10;
        n=n/10;
        s=s+d;
    }
    printf("%d",s);
    }
