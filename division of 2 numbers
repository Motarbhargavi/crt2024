#include <stdio.h>

int main() {
    // Write C code here
    int a,b;
    scanf("%d%d",&a,&b);
    int r=a/b;
    printf("%d\n",r);
    return 0;
}
#include <stdio.h>

int main() {
    // Write C code here
    int a,b;
    scanf("%d%d",&a,&b);
   if(b==0)
   {
       printf("second no cant be zero");
       b=1;
   }
   int r=a/b;
    printf("%d",r);
    return 0;
}
